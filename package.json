{
  "name": "book-meeting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm:dev\" \"npm:server\" ",
    "server": "npm --prefix ./server run dev",
    "dev": "vite --open --host --port 1812",
